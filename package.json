{
  "name": "react-with-timer-hoc",
  "description": "A React HOC that simplifies setTimeout-based timers.",
  "version": "0.1.1",
  "author": "Jay Cheroske <jay@portal.org>",
  "repository": {
    "type": "git",
    "url": "git://github.com/jcheroske/react-with-timer-hoc.git"
  },
  "bugs": {
    "url": "https://github.com/jcheroske/react-with-timer-hoc/issues"
  },
  "homepage": "https://github.com/jcheroske/react-with-timer-hoc#readme",
  "license": "MIT",
  "keywords": [
    "react",
    "higher-order",
    "components",
    "hoc",
    "recompose",
    "utilities",
    "setTimeout"
  ],
  "main": "./dist/index.js",
  "scripts": {
    "build": "NODE_ENV=development webpack --progress --colors",
    "build:prod": "NODE_ENV=production webpack --progress --colors",
    "build:watch": "NODE_ENV=development webpack --progress --colors --watch",
    "clean": "rimraf dist/*",
    "modules": "rimraf node_modules/* && npm i",
    "test": "ava --concurrency=5",
    "test:watch": "npm run test -- --watch",
    "prepublish": "npm run build:prod",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix"
  },
  "files": [
    "dist"
  ],
  "dependencies": {},
  "peerDependencies": {
    "invariant": "^2.2.2",
    "lodash": "^4.17.4",
    "react": "^15.4.2"
  },
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.4.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-jsx": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.2.1",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.24.0",
    "enzyme": "^2.7.1",
    "eslint": "^3.17.1",
    "eslint-config-standard": "^7.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.0",
    "eslint-plugin-standard": "^2.1.1",
    "invariant": "^2.2.2",
    "jsdom": "^9.12.0",
    "lodash": "^4.17.4",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "rimraf": "^2.6.1",
    "sinon": "^1.17.7",
    "source-map-loader": "^0.2.0",
    "uglifyjs-webpack-plugin": "^0.3.0",
    "webpack": "^2.2.1"
  },
  "devEngines": {
    "node": "5.x",
    "npm": "3.x"
  },
  "ava": {
    "files": [
      "src/**/__tests__/*{.,-}test.js"
    ],
    "source": [
      "src/**/*.js"
    ],
    "failFast": true,
    "require": [
      "babel-register",
      "./ava-test-setup"
    ],
    "babel": "inherit"
  }
}
